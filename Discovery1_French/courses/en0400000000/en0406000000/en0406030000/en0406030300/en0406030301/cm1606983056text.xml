<component id="applicationTitle">
	<text><![CDATA[<b>Application Layer</b>]]></text>
</component>
<component id="applicationBody">
	<text><![CDATA[<li>Defines interfaces between application software and network communication functions.</li><li>Provides standardized services such as file transfer between systems.</li>]]></text>
</component>
<component id="presentationTitle">
	<text><![CDATA[<b>Presentation Layer</b>]]></text>
</component>
<component id="presentationBody">
	<text><![CDATA[<li>Standardizes user data formats for use between different types of systems.</li><li>Encodes and decodes user data; encrypts and decrypts data; compresses and decompresses data.</li>]]></text>
</component>
<component id="sessionTitle">
	<text><![CDATA[<b>Session Layer</b>]]></text>
</component>
<component id="sessionBody">
	<text><![CDATA[<li>Manages user sessions and dialogues.</li><li>Maintains logical links between systems.</li>]]></text>
</component>
<component id="transportTitle">
	<text><![CDATA[<b>Transport Layer</b>]]></text>
</component>
<component id="transportBody">
	<text><![CDATA[<li>Manages end-to-end message delivery over the network.</li><li>Can provide reliable and sequential packet delivery through error recovery and flow control mechanisms.</li>]]></text>
</component>
<component id="networkTitle">
	<text><![CDATA[<b>Network Layer</b>]]></text>
</component>
<component id="networkBody">
	<text><![CDATA[<li>Routes packets according to unique network device addresses.</li>]]></text>
</component>
<component id="data_title">
	<text><![CDATA[<b>Data Link Layer</b>]]></text>
</component>
<component id="data_body">
	<text><![CDATA[<li>Defines procedures for operating the communication links.</li><li>Detects and corrects frame transmit errors.</li>]]></text>
</component>
<component id="physicalTitle">
	<text><![CDATA[<b>Physical Layer</b>]]></text>
</component>
<component id="physicalBody">
	<text><![CDATA[<li>Defines physical means of sending data over network devices.</li><li>Interfaces between network medium and devices.</li><li>Defines optical, electrical and mechanical characteristics.</li>]]></text>
</component>
