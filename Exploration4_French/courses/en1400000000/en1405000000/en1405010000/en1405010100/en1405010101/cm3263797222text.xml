<!-- slide 1-->

<component id="ID_txt03">
<text><![CDATA[&quot;Let's talk.&quot;]]></text>
</component>

<component id="ID_txt04">
<text><![CDATA[TCP SYN packet]]></text>
</component>

<component id="ID_txt05">
<text><![CDATA[&quot;Sure, let's talk.&quot;]]></text>
</component>

<component id="ID_txt06">
<text><![CDATA[TCP SYN/ACK packet]]></text>
</component>

<component id="ID_txt07">
<text><![CDATA[&quot;Great, we have a connection.&quot;]]></text>
</component>

<component id="ID_txt08">
<text><![CDATA[TCP ACK packet]]></text>
</component>

<component id="ID_txt09">
<text><![CDATA[&quot;I need you to send me some data.&quot;]]></text>
</component>

<component id="ID_txt10">
<text><![CDATA[TCP data segment]]></text>
</component>

<component id="ID_txt11">
<text><![CDATA[&quot;Thanks, I received your request.&quot;]]></text>
</component>

<component id="ID_txt12">
<text><![CDATA[TCP ACK packet]]></text>
</component>

<component id="ID_txt13">
<text><![CDATA[&quot;Here is the data you want.&quot;]]></text>
</component>

<component id="ID_txt14">
<text><![CDATA[TCP data segment(s)]]></text>
</component>

<component id="ID_txt15">
<text><![CDATA[&quot;Thanks. I received my data.&quot;]]></text>
</component>

<component id="ID_txt16">
<text><![CDATA[TCP ACK packet]]></text>
</component>

<component id="ID_txt17">
<text><![CDATA[&quot;I am finished and have no more data to send.&quot;]]></text>
</component>

<component id="ID_txt18">
<text><![CDATA[TCP FIN/ACK packet]]></text>
</component>

<component id="ID_txt19">
<text><![CDATA[&quot;I am finished as well. Thanks.&quot;]]></text>
</component>

