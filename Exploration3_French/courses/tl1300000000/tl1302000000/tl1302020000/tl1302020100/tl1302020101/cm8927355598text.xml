<component id="ID_txt03">
  <text><![CDATA[<B>La trame est acheminée via le commutateur avant d’être entièrement reçue.</B>]]></text> 
</component>

<component id="ID_txt04">
  <text><![CDATA[<B>La trame complète est reçue avant d’être transmise.</B>]]></text> 
</component>

