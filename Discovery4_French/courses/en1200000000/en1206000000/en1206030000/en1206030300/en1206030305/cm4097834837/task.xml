	<hwcomponents>
      <component type='router' model='2624' id='dev_post'>
         <card type='fastethernet' slot='0' fixed='false' >
            <port type='fastethernet' number='0' name='FastEthernet0/0' id='dev_post_fa00' fixed='false' />
         </card>
         <card type='serial' slot='0' fixed='false' >
            <port type='serial' number='0' name='Serial0/0' id='dev_post_s00' fixed='false' />
            <port type='serial' number='1' name='Serial0/1' id='dev_post_s01' fixed='false' />
         </card>
         <config file='startup-config'> 
<![CDATA[
!
version 12.2
!
hostname Post
!
!
!
interface FastEthernet0/0
 no ip address
 shutdown
!
interface Serial0/0
 ip address 192.168.1.1 255.255.255.0
 clock rate 56000
!
interface Serial0/1
 no ip address
 shutdown
!
!
router eigrp 100
 network 192.168.1.0
 auto-summary
!
!
!
line con 0
line vty 0 4
!
end
]]>
		 </config>
      </component>

      <component type='router' model='2624' id='dev_york'>
         <card type='fastethernet' slot='0' fixed='false' >
            <port type='fastethernet' number='0' name='FastEthernet0/0' id='dev_york_fa00' fixed='false' />
         </card>
         <card type='serial' slot='0' fixed='false' >
            <port type='serial' number='0' name='Serial0/0' id='dev_york_s00' fixed='false' />
            <port type='serial' number='1' name='Serial0/1' id='dev_york_s01' fixed='false' />
         </card>
         <config file='startup-config'> 
<![CDATA[
!
version 12.2
!
hostname York
!
!
!
interface FastEthernet0/0
 no ip address
 shutdown
!
interface Serial0/0
 ip address 192.168.1.2 255.255.255.0
!
interface Serial0/1
 ip address 192.168.2.1 255.255.255.0
 clock rate 56000
!
!
router eigrp 100
 network 192.168.1.0
 network 192.168.2.0
 auto-summary
!
!
!
line con 0
line vty 0 4
!
end
]]>
		 </config>
      </component>

      <component type='router' model='2624' id='dev_melk'>
         <card type='fastethernet' slot='0' fixed='false' >
