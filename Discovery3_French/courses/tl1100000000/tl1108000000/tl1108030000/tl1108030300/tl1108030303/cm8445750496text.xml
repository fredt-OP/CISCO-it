	<component id="ID_object1">
		<text><![CDATA[ip access-group 44 out]]></text>
	</component>
	<component id="ID_object2">
		<text><![CDATA[access-list 44 deny any]]></text>
	</component>
	<component id="ID_object3">
		<text><![CDATA[access-list 44 permit 192.168.3.0 0.0.0.255]]></text>
